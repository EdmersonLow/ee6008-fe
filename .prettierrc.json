{
	"tabWidth": 4,
	"useTabs": true,
	"printWidth": 100,
	"semi": true,
	"trailingComma": "es5",
	"singleQuote": true,
	"endOfLine": "lf",
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": ["^@/(?!components|app/).*", "^@/components/(.*)$", "^@/app/(.*)$", "^[./]"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
